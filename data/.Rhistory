p_values <- shapiro.test(x[,2])$p.value
}
ind_n <- p_values>.05
vec <- names(x[,-1])[ind_n]
if(length(vec)>0){
fit <- lm(x[,1]~., data= x[,vec])
return(fit$coefficients)
}else{
return("There are no normal variables in the data")
}
}
